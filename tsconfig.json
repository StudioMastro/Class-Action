{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["ES2017", "DOM"],
    "typeRoots": ["./node_modules/@figma", "./node_modules/@types"],
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "outDir": ".",
    "rootDir": "./src"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}